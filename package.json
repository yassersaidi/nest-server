    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.0",
    "keyv": "^5.1.3",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "resend": "^4.0.0",
    "rxjs": "^7.8.1",
    "twilio": "^5.3.5",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.0.0",
