    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@types/ua-parser-js": "^0.7.39",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
